[
  {
    "route": "/address",
    "factory": "Addresses\\Factory\\AddressControllerFactory",
    "action": "getAddresses",
    "method": "GET"
  },
  {
    "route": "/address",
    "factory": "Addresses\\Factory\\AddressControllerFactory",
    "action": "addAddresses",
    "method": "POST"
  },
  {
    "route": "/address/:id",
    "factory": "Addresses\\Factory\\AddressControllerFactory",
    "method": "GET",
    "action": "getAddress",
    "params": {
      ":id": "([0-9]+)"
    }
  },
  {
    "route": "/address/:id",
    "factory": "Addresses\\Factory\\AddressControllerFactory",
    "method": "PUT",
    "action": "updateAddress",
    "params": {
      ":id": "([0-9]+)"
    }
  }
]
